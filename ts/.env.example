# ============================================================
# Vercel Gateway 配置 - TypeScript 版本
# ============================================================

# 服务器配置
PORT=3001
HOST=0.0.0.0

# 认证密钥（用于保护 API）
AUTH_KEY=your-secret-key-here

# 密钥文件配置
KEYS_DIR=./data/keys
KEYS_FILE=keys_high.txt

# 冷却时间（小时）
COOLDOWN_HOURS=24

# 最大重试次数
MAX_RETRIES=3

# 上游 Vercel AI Gateway
UPSTREAM_GATEWAY=https://ai-gateway.vercel.sh

# 模型列表缓存时间（秒）
MODELS_CACHE_TTL=3600

# 日志级别
LOG_LEVEL=info
