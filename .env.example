# Vercel Gateway 配置

# 上游 Vercel AI Gateway 地址
VERCEL_GATEWAY_URL=https://ai-gateway.vercel.sh

# 代理服务监听端口
PORT=3001

# 认证密钥（客户端调用时需要）
AUTH_KEY=your-auth-key-here

# 密钥冷却时间（小时）
COOLDOWN_HOURS=24

# 密钥文件路径
KEYS_FILE=data/keys/keys_high.txt

# 冷却密钥记录文件
COOLDOWN_FILE=data/keys/cooldown_keys.json

# 日志目录
LOG_DIR=logs

# 是否启用 Cherry Studio 参数转换
# 启用后会自动转换 providerOptions 中的思考强度等参数
ENABLE_PARAMS_CONVERSION=false

# 模型列表缓存时间（秒），默认 3600 秒（1小时）
MODELS_CACHE_TTL=3600
